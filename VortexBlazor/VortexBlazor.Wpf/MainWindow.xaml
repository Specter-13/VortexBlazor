<Window x:Class="VortexBlazor.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VortexBlazor.Wpf"
        xmlns:vortex="http://vortex.mts/xaml"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="857">
    <Window.DataContext>
        <local:MainWindowViewModel />
    </Window.DataContext>
    <Grid>
        <StackPanel>
            <vortex:RenderableContentControl DataContext="{Binding PLC.TECH_MAIN._app._station001._renderIgnoreTests._renderIgnore_basic, Mode=OneWay}" PresentationType="ShadowDisplay" RenderTransformOrigin="0.496,0.549">
             
            </vortex:RenderableContentControl>
        </StackPanel>
    </Grid>
</Window>
